 p_id=>wwv_flow_imp.id(5972055811752622559)
,p_chart_id=>wwv_flow_imp.id(5972054126468622558)
,p_seq=>10
,p_name=>'Series 1'
,p_data_source_type=>'SQL'
,p_data_source=>wwv_flow_string.join(wwv_flow_t_varchar2(
'select YEAR, "NO._OF_REFUGEES"',
'from GLOBAL_REFUGEE',
'where "NO._OF_REFUGEES" is not null',
'order by 2 asc'))
,p_max_row_count=>40
,p_items_value_column_name=>'NO._OF_REFUGEES'
